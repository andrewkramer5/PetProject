<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<Pane fx:id="canvasPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Canvas fx:id="canvas" height="400.0" width="500.0" />
      <VBox layoutX="175.0" layoutY="175.0" spacing="20.0">
         <children>
            <Button fx:id="newPet" focusTraversable="false" minHeight="50.0" minWidth="150.0" mnemonicParsing="false" text="NEW PET" />
            <Button fx:id="loadPet" focusTraversable="false" minHeight="50.0" minWidth="150.0" mnemonicParsing="false" text="LOAD PET" />
            <Button fx:id="exit" focusTraversable="false" minHeight="50.0" minWidth="150.0" mnemonicParsing="false" text="EXIT" />
         </children>
      </VBox>
   </children>
</Pane>
